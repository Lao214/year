<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.year.dao.SunshineMapper">

    <select id="getOneByJobNo" parameterType="java.lang.String" resultType="com.example.year.entity.Sunshine">
        select * from sunshine
        where jobno = #{jobNo} limit 1;
    </select>
</mapper>
